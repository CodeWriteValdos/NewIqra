<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rplwikrama.a7win64.newiqra"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="28" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.rplwikrama.a7win64.newiqra.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.rplwikrama.a7win64.newiqra.MainActivity" />
        <activity
            android:name="com.rplwikrama.a7win64.newiqra.RegisterActivity"
            android:label="@string/regis"
            android:parentActivityName="com.rplwikrama.a7win64.newiqra.LoginActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LoginActivity" />
        </activity>
        <activity
            android:name="com.rplwikrama.a7win64.newiqra.RiwayatNTActivity"
            android:label="@string/btnRiwayatNT" >
        </activity>
        <activity
            android:name="com.rplwikrama.a7win64.newiqra.LaporanKlaimActivity"
            android:label="@string/laporanKlaim" >
        </activity>
        <activity
            android:name="com.rplwikrama.a7win64.newiqra.RiwayatPKActivity"
            android:label="@string/riwayatPK" >
        </activity>
    </application>

</manifest>