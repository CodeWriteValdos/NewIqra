                    android:layout_height="wrap_content"
                    android:text="@string/titik_dua"
                    android:paddingEnd="5dp" />

                <TextView
                    android:id="@+id/lblArray"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="" />

            </TableRow>

            <TableRow
                android:padding="5dp">

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="@string/jthTempo" />

