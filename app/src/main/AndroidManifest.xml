<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rplwikrama.a7win64.newiqra">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/regis"
            android:parentActivityName=".LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LoginActivity" />
        </activity>
        <activity
            android:name=".RiwayatNTActivity"
            android:label="@string/btnRiwayatNT">
        </activity>
        <activity
            android:name=".LaporanKlaimActivity"
            android:label="@string/laporanKlaim">
        </activity>
        <activity
            android:name=".RiwayatPKActivity"
            android:label="@string/riwayatPK">
        </activity>
    </application>

</manifest>